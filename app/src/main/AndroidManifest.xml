        </activity>
        <activity android:name=".PlayerList" />
        <activity android:name=".ShowPlayerStats" />
        <activity android:name=".TeamLeader" />
        <activity android:name=".TeamSchedule" />
        <activity android:name=".TeamRank"></activity>
    </application>

</manifest>